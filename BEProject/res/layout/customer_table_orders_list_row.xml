<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="15dp"
    android:paddingRight="8dp"
    android:paddingTop="10dp"
    android:paddingBottom="10dp"
    android:orientation="vertical"
    
    android:background="@drawable/customshapeforcustomertableorder">
    
	<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:weightSum="3">
    <TextView
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="wrap_content"
        android:text="Table Order Id:" />

    <TextView
        android:id="@+id/customerTableOrderIdTextView"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:text="TextView" 
        />
	</LinearLayout>
    <LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:weightSum="3">

    <TextView
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="wrap_content"
        android:text="Number Of Guests:" />

    <TextView
        android:id="@+id/customerTableOrderNoOfSeatsTextView"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:text="TextView" />
    </LinearLayout>
	<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:weightSum="3">
    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="wrap_content"
        android:text="Time:" />

    <TextView
        android:id="@+id/customerTableOrderTimeTextView"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:text="TextView" />
    </LinearLayout>
	<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:weightSum="3">
    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="wrap_content"
        android:text="Date:" />

    <TextView
        android:id="@+id/customerTableOrderDateTextView"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:text="TextView" />
    </LinearLayout>
	<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:weightSum="3">
    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="wrap_content"
        android:text="Restaurant Id:" />

    <TextView
        android:id="@+id/customerTableOrderRestaurantIdTextView"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:text="TextView" />
    </LinearLayout>
	<LinearLayout
	    android:weightSum="3"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="wrap_content"
        android:text="Restaurant Name:" />

    <TextView
        android:id="@+id/customerTableOrderRestaurantNameTextView"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:text="TextView" />
    </LinearLayout>
	<LinearLayout
    android:layout_width="fill_parent"
    android:weightSum="3"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <TextView
        android:id="@+id/textView13"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="wrap_content"
        android:text="Base Payment:" />

    <TextView
        android:id="@+id/customerTableOrderBasePaymentTextView"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:text="TextView" />
	</LinearLayout>
<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:weightSum="3">
	<TextView
	    android:id="@+id/textView1"
	    android:layout_width="0dp"
        android:layout_weight="2"
	    android:layout_height="wrap_content"
	    android:text="status" />

	<TextView
	    android:id="@+id/customerTableOrderFlag"
	    android:layout_width="0dp"
        android:layout_weight="1"
	    android:layout_height="wrap_content"
	    android:text="TextView" 
	    android:textStyle="italic"
	    android:textSize="18dp"
	    android:textColor="#483D8B"/>
</LinearLayout>
</LinearLayout>
